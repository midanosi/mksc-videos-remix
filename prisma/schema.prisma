generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model mkscvids {
  id           Int
  cid          Int
  time         Float
  link         String @unique
  mode         Int
  player       String
  uploaded_at  DateTime?
  standard     String?
  // course Course @relation(fields: [cid], references: [cid])
  // standard Standard @relation(fields: [cid], references: [cid])
}

